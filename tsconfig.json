{
	"extends": "@quasar/app-vite/tsconfig-preset",
	"compilerOptions": {
		"baseUrl": ".",
		"lib": ["ES6", "ESNext"],
		"paths": {
			"components/*": ["./src/components/*"],
			"@interfaces/*": ["./src/interfaces/*"]
		}
	},
	"exclude": [
		"./dist",
		"./.quasar",
		"./node_modules",
		"./src-capacitor",
		"./src-cordova",
		"./quasar.config.*.temporary.compiled*"
	]
}
